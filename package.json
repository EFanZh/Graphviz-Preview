{
    "name": "graphviz-preview",
    "version": "1.3.4",
    "publisher": "EFanZh",
    "engines": {
        "vscode": "^1.41.1"
    },
    "license": "MIT",
    "displayName": "Graphviz Preview",
    "description": "Preview Graphviz (DOT) files.",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "dot",
        "graphviz",
        "preview"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "dot",
                "aliases": [
                    "Graphviz",
                    "DOT"
                ],
                "extensions": [
                    ".dot",
                    ".gv"
                ]
            }
        ],
        "commands": [
            {
                "command": "graphviz.showPreviewToSide",
                "title": "Open Preview to the Side",
                "category": "Graphviz",
                "icon": {
                    "dark": "resources/show-preview-dark.svg",
                    "light": "resources/show-preview-light.svg"
                }
            }
        ],
        "configuration": {
            "title": "Graphviz Preview",
            "properties": {
                "graphvizPreview.dotPath": {
                    "description": "The path of the dot program.",
                    "type": [
                        "string",
                        "null"
                    ],
                    "default": null
                },
                "graphvizPreview.engine": {
                    "description": "The layout engine to use.",
                    "type": [
                        "string",
                        "null"
                    ],
                    "enum": [
                        "dot",
                        null
                    ],
                    "default": null
                }
            }
        },
        "menus": {
            "editor/title": [
                {
                    "command": "graphviz.showPreviewToSide",
                    "group": "navigation",
                    "when": "editorLangId == dot"
                }
            ]
        },
        "keybindings": [
            {
                "command": "graphviz.showPreviewToSide",
                "key": "ctrl+k v",
                "mac": "cmd+k v",
                "when": "editorLangId == dot"
            }
        ]
    },
    "activationEvents": [
        "onCommand:graphviz.showPreviewToSide"
    ],
    "badges": [
        {
            "url": "https://github.com/EFanZh/Graphviz-Preview/workflows/.github/workflows/CI.yml/badge.svg",
            "href": "https://github.com/EFanZh/Graphviz-Preview/actions?query=workflow%3A.github%2Fworkflows%2FCI.yml",
            "description": "Build Status"
        }
    ],
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.1.8",
        "@types/vscode": "^1.41.0",
        "@typescript-eslint/eslint-plugin": "^2.16",
        "@typescript-eslint/parser": "^2.16",
        "concurrently": "^5.0.2",
        "eslint": "^6.8",
        "glob": "^7.1.6",
        "mocha": "^7.0.0",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
    },
    "scripts": {
        "vscode:prepublish": "npm run lint && npm run compile",
        "compile": "tsc -p . && webpack",
        "watch": "concurrently --kill-others --allow-restart --restart-after 500 -r \"tsc -p . -w\" \"webpack -w\"",
        "lint": "eslint src/**/*.ts",
        "pretest": "npm run lint && npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "icon": "resources/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/EFanZh/Graphviz-Preview"
    }
}
